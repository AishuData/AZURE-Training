{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Order_ID as string,",
				"          Customer_Name as string,",
				"          Category as string,",
				"          Sub_Category as string,",
				"          City as string,",
				"          Order_Date as date,",
				"          Region as string,",
				"          Sales as integer,",
				"          Discount as timestamp,",
				"          Profit as double,",
				"          State as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source1 select(mapColumn(",
				"          Order_ID,",
				"          Customer_Name,",
				"          Category,",
				"          Sub_Category,",
				"          City,",
				"          Order_Date,",
				"          Region,",
				"          Sales,",
				"          Discount,",
				"          Profit,",
				"          State",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}